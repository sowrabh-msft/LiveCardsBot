{
  "type": "AdaptiveCard",
  "body": [
    {
      "type": "TextBlock",
      "text": "Request initiated by:",
      "size": "Medium",
      "weight": "Bolder"
    },
    {
      "type": "TextBlock",
      "text": "${createdBy}",
      "size": "Medium",
      "weight": "Bolder"
    },
    {
      "type": "ActionSet",
      "actions": [
        {
          "type": "Action.Execute",
          "verb": "initiateSSO",
          "title": "CheckForSSO",
          "data": {
            "CreatedBy": "${createdBy}"
          }
        },
        {
          "type": "Action.Execute",
          "verb": "initiateOAuth",
          "title": "CheckForOAuth",
          "data": {
            "CreatedBy": "${createdBy}"
          }
        },
        {
          "type": "Action.Execute",
          "verb": "loopSSO",
          "title": "LoopingSSO",
          "data": {
            "CreatedBy": "${createdBy}"
          }
        },
        {
          "type": "Action.Execute",
          "verb": "loopOAuth",
          "title": "LoopingOAuth",
          "data": {
            "CreatedBy": "${createdBy}"
          }
        }
      ]
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.4"
}
